---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>orta therox</title>
 <link href="http://orta.github.com/blog.atom" rel="self"/>
 <link href="http://orta.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://orta.github.com/</id>
 <author>
   <name>orta therox</name>
   <email>orta.therox@gmail.com</email>
 </author>
 {% for post in site.posts limit: 15 %}
 <entry>
   <title type="html">{{ post.title | xml_escape }}</title>
   <link rel="alternate" href="http://orta.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://orta.github.com{{ post.id }}</id>
   <content type="xhtml" xml:lang="{% if post.lang %}{{ post.lang }}{% else %}en{% endif %}" xml:base="http://orta.github.com/">
    <div xmlns="http://www.w3.org/1999/xhtml">
     {{ post.content }}
    </div>
   </content>
 </entry>
 {% endfor %}
</feed>